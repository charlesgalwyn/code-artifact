{
  "data": {
    "filename": "share-student-activity-data.csv",
    "sequence_length": 50,
    "train_test_split": 0.85,
    "normalise": true,
    "numeric_columns": ["Duration", "LA"],
    "label_col": "Phase"
  },
  "model": {
    "rnn_type": "lstm",
    "bidirectional": false,
    "rnn_units": 128,
    "rnn_return_seq": false,
    "dropout": 0.2,
    "dense_units": 64,
    "optimizer": "adam",
    "embeddings": {
      "action_dim": 32,
      "reg_dim": 8,
      "self_dim": 4
    },
    "save_dir": "saved_models"
  },
  "training": {
    "epochs": 8,
    "batch_size": 32,
    "validation_split": 0.1
  }
}
